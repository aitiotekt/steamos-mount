{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "steamos-mount-tauri",
  "version": "0.1.2",
  "identifier": "com.aitiotekt.steamos-mount-tauri",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "steamos-mount-tauri",
        "width": 640,
        "height": 600,
        "minWidth": 500,
        "minHeight": 400,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "binaries/": "bin/"
    }
  }
}